{"ast":null,"code":"export default {\n  data() {\n    return {\n      TatCa: null\n    };\n  },\n\n  props: ['chonPhim'],\n\n  created() {\n    fetch(\"https://api.apify.com/v2/key-value-stores/QubTry45OOCkTyohU/records/LATEST?fbclid=IwAR2CQrkSmpdayjUmuQDm2BmuZtaSBm0ol5PoNNEvzxo5UI8AwacyymiJ3B8\").then(response => response.json()).then(data => this.TatCa = data.phim.phimchieurap).then(() => {\n      console.log(this.TatCa);\n    });\n  },\n\n  methods: {\n    chonP() {\n      console.log(this.chonPhim);\n    }\n\n  },\n  computed: {\n    PCoTrang(phim) {\n      return phim.filter(p => p.category == \"Phim cổ trang\");\n    },\n\n    PHanhDong(phim) {\n      return phim.filter(p => p.category == \"Phim hành động\");\n    },\n\n    PHoatHinh(phim) {\n      return phim.filter(p => p.category == \"Phim hoạt hình\");\n    },\n\n    PKinhDi(phim) {\n      return phim.filter(p => p.category == \"Phim kinh dị\");\n    },\n\n    PTinhCam(phim) {\n      return phim.filter(p => p.category == \"Phim tinh cảm\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAeA,eAAe;EACXA,IAAI,GAAE;IACF,OAAO;MACHC,KAAK,EAAE;IADJ,CAAP;EAIH,CANU;;EAOXC,KAAK,EAAE,CAAC,UAAD,CAPI;;EAQXC,OAAO,GAAE;IACLC,KAAK,CAAC,iJAAD,CAAL,CACCC,IADD,CACOC,QAAD,IAAYA,QAAQ,CAACC,IAAT,EADlB,EAECF,IAFD,CAEOL,IAAD,IAAS,KAAKC,KAAL,GAAaD,IAAI,CAACQ,IAAL,CAAUC,YAFtC,EAGCJ,IAHD,CAGM,MAAI;MAAEK,OAAO,CAACC,GAAR,CAAY,KAAKV,KAAjB;IAAwB,CAHpC;EAIH,CAbU;;EAcXW,OAAO,EAAC;IACJC,KAAK,GAAE;MACHH,OAAO,CAACC,GAAR,CAAY,KAAKG,QAAjB;IACJ;;EAHI,CAdG;EAmBXC,QAAQ,EAAC;IACLC,QAAQ,CAACR,IAAD,EAAM;MACV,OAAOA,IAAI,CAACS,MAAL,CAAYC,CAAC,IAAGA,CAAC,CAACC,QAAF,IAAc,eAA9B,CAAP;IACH,CAHI;;IAILC,SAAS,CAACZ,IAAD,EAAM;MACX,OAAOA,IAAI,CAACS,MAAL,CAAYC,CAAC,IAAGA,CAAC,CAACC,QAAF,IAAc,gBAA9B,CAAP;IACH,CANI;;IAOLE,SAAS,CAACb,IAAD,EAAM;MACX,OAAOA,IAAI,CAACS,MAAL,CAAYC,CAAC,IAAGA,CAAC,CAACC,QAAF,IAAc,gBAA9B,CAAP;IACH,CATI;;IAULG,OAAO,CAACd,IAAD,EAAM;MACT,OAAOA,IAAI,CAACS,MAAL,CAAYC,CAAC,IAAGA,CAAC,CAACC,QAAF,IAAc,cAA9B,CAAP;IACH,CAZI;;IAaLI,QAAQ,CAACf,IAAD,EAAM;MACV,OAAOA,IAAI,CAACS,MAAL,CAAYC,CAAC,IAAGA,CAAC,CAACC,QAAF,IAAc,eAA9B,CAAP;IACH;;EAfI;AAnBE,CAAf","names":["data","TatCa","props","created","fetch","then","response","json","phim","phimchieurap","console","log","methods","chonP","chonPhim","computed","PCoTrang","filter","p","category","PHanhDong","PHoatHinh","PKinhDi","PTinhCam"],"sourceRoot":"","sources":["C:\\Users\\ASUS\\Desktop\\movieclone\\src\\view\\PhimAll.vue"],"sourcesContent":["<template>\r\n    <div class=\"wap\" chonPhimm >  \r\n        <div class=\"flimWarp\" v-for=\"tatca in TatCa\" :key=\"tatca.imageUrl\" >\r\n            <div class=\"HinhAnh\">\r\n                <img :src=\"tatca.imageUrl\" class=\"\" alt=\"\">\r\n            </div>\r\n            <div class=\"TenPhim\">\r\n                <h4>{{tatca.title}}</h4>\r\n                <p>{{tatca.category}}</p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    \r\n</template>\r\n<script>    \r\nexport default {\r\n    data(){\r\n        return {\r\n            TatCa: null,\r\n            \r\n        }\r\n    },\r\n    props: ['chonPhim'],\r\n    created(){ \r\n        fetch(\"https://api.apify.com/v2/key-value-stores/QubTry45OOCkTyohU/records/LATEST?fbclid=IwAR2CQrkSmpdayjUmuQDm2BmuZtaSBm0ol5PoNNEvzxo5UI8AwacyymiJ3B8\")\r\n        .then((response)=>response.json())\r\n        .then((data)=>(this.TatCa = data.phim.phimchieurap))\r\n        .then(()=>{ console.log(this.TatCa)})\r\n    },\r\n    methods:{\r\n        chonP(){\r\n            console.log(this.chonPhim)\r\n        }\r\n    },\r\n    computed:{\r\n        PCoTrang(phim){\r\n            return phim.filter(p=> p.category == \"Phim cổ trang\")\r\n        },\r\n        PHanhDong(phim){\r\n            return phim.filter(p=> p.category == \"Phim hành động\")\r\n        },\r\n        PHoatHinh(phim){\r\n            return phim.filter(p=> p.category == \"Phim hoạt hình\")\r\n        },\r\n        PKinhDi(phim){\r\n            return phim.filter(p=> p.category == \"Phim kinh dị\")\r\n        },\r\n        PTinhCam(phim){\r\n            return phim.filter(p=> p.category == \"Phim tinh cảm\")\r\n        },\r\n    }\r\n  \r\n}\r\n</script>\r\n<style scoped>\r\n@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&display=swap');\r\n.wap{\r\n    display: flex;\r\n    flex-direction: row;  \r\n    flex-wrap: wrap ;\r\n}\r\n.flimWarp{\r\n    position: relative;\r\n    height: 600px ;\r\n    width: 22%;\r\n    margin: 10px;\r\n    background-color: #605F7B;\r\n    border-radius: 10px ;\r\n    box-sizing: border-box;\r\n    z-index: 1;\r\n    cursor: pointer;\r\n    overflow: hidden;\r\n}\r\n.HinhAnh{\r\n    width: 100%;\r\n    height: 400px;\r\n    border-radius: 10px 10px 0px 0px;\r\n    transition: 0.5s ;\r\n    overflow: hidden;\r\n}\r\n.HinhAnh>img{\r\n    width: 100%;\r\n    height: 400px;\r\n    border-radius: 10px 10px 0px 0px;\r\n    transition: 0.5s ;\r\n    overflow: hidden;\r\n}\r\n.flimWarp:hover {\r\n    background-color: #484681;\r\n}\r\n.HinhAnh>img:hover{\r\n    transform: scale(1.1);\r\n    overflow: hidden;\r\n}\r\n.TenPhim{\r\n    height: 200px;\r\n}\r\n.TenPhim>h4{\r\n    color: white;\r\n    font-size: 20px;\r\n    text-decoration: none;\r\n    font-family: 'Open Sans', sans-serif;\r\n    margin-left: 8px;\r\n    margin-right: 8px;\r\n    margin-top: 20px;   \r\n}\r\n.TenPhim>p{\r\n    position: absolute;\r\n    color: white;\r\n    font-size: 20px;\r\n    text-decoration: none;\r\n    font-family: 'Open Sans', sans-serif;\r\n    bottom: 10px;\r\n    left:8px    \r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}