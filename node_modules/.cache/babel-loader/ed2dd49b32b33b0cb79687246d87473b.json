w\">\r\n            <h3>Phim mới</h3>\r\n            <div v-if=\"chonTheLoai === 'all'\">\r\n                <div class=\"wap-hienthi\">  \r\n                    <div class=\"flimWarp\" v-for=\"tatca in TatCa\" :key=\"tatca.imageUrl\"  @click=\"chonPhimHienThi(tatca.imageUrl,tatca.title,tatca.category,tatca.url)\">\r\n                        <div class=\"HinhAnh\">\r\n                            <img :src=\"tatca.imageUrl\" class=\"\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"TenPhim\">\r\n                            <h4>{{tatca.title}}</h4>\r\n                            <p>{{tatca.category}}</p>\r\n                        </div>\r\n                    </div>\r\n                    \r\n                </div>\r\n            </div>\r\n            <div v-else-if=\"chonTheLoai === 'ct'\">\r\n                <div class=\"wap-hienthi\">  \r\n                    <div class=\"flimWarp\" v-for=\"tatca in PCoTrang\" :key=\"tatca.imageUrl\" >\r\n                        <div class=\"HinhAnh\">\r\n                            <img :src=\"tatca.imageUrl\" class=\"\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"TenPhim\">\r\n                            <h4>{{tatca.title}}</h4>\r\n                            <p>{{tatca.category}}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div v-else-if=\"chonTheLoai === 'kd'\">\r\n                <div class=\"wap-hienthi\">  \r\n                    <div class=\"flimWarp\" v-for=\"tatca in PKinhDi\" :key=\"tatca.imageUrl\" >\r\n                        <div class=\"HinhAnh\">\r\n                            <img :src=\"tatca.imageUrl\" class=\"\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"TenPhim\">\r\n                            <h4>{{tatca.title}}</h4>\r\n                            <p>{{tatca.category}}</p>\r\n                        </div>\